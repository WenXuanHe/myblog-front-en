{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "baseUrl": "./",
        "paths": {
            "$boundle": ["public/src/helper/boundle"]
        }
    },
    "include": [
        "./public/src/**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}